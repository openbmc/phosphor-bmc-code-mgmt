[project]
	description = Manage the BMC's code versions.
[access]
	inheritFrom = openbmc-cpp-repos
[submit]
	action = rebase if necessary
[access "refs/heads/*"]
	create = group github/openbmc/phosphor-bmc-code-mgmt-maintainers
	forgeCommitter = group github/openbmc/phosphor-bmc-code-mgmt-maintainers
	push = group github/openbmc/phosphor-bmc-code-mgmt-maintainers
	label-Code-Review = -2..+2 group github/openbmc/phosphor-bmc-code-mgmt-maintainers
	label-Ok-To-Test = +0..+1 group github/openbmc/phosphor-bmc-code-mgmt-maintainers
	label-Verified = -1..+1 group github/openbmc/phosphor-bmc-code-mgmt-maintainers
	submit = group github/openbmc/phosphor-bmc-code-mgmt-maintainers
	editTopicName = group github/openbmc/phosphor-bmc-code-mgmt-maintainers
[access "refs/tags/*"]
	push = +force group github/openbmc/phosphor-bmc-code-mgmt-maintainers
	pushTag = +force group github/openbmc/phosphor-bmc-code-mgmt-maintainers
	pushSignedTag = group github/openbmc/phosphor-bmc-code-mgmt-maintainers
